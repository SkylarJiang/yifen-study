<view class='page-box'>
  <van-cell-group>

<van-field
  value="{{ account }}"
  label="学号"
  placeholder="请输入学号"
  required
  data-name="account"
  bind:change="onFieldChange"
  error-message="{{accountFalse?accountFalseMess:''}}"
/>

<van-field
  value="{{ password }}"
  label="姓名"
  placeholder="请输入姓名"
  data-name="password"
  type="password"
  required
  bind:change="onFieldChange"
  error-message="{{passwordFalse?passwordFalseMess:''}}"
/>

<van-field
  value="{{ password }}"
  label="班级"
  placeholder="请输入班级"
  data-name="password"
  type="password"
  required
  bind:change="onFieldChange"
  error-message="{{passwordFalse?passwordFalseMess:''}}"
/>
<van-field
  value="{{ password }}"
  label="专业"
  placeholder="请输入专业"
  data-name="password"
  type="password"
  required
  bind:change="onFieldChange"
  error-message="{{passwordFalse?passwordFalseMess:''}}"
/>

<van-field
  value="{{ password }}"
  label="密码"
  placeholder="请输入密码"
  data-name="password"
  type="password"
  required
  bind:change="onFieldChange"
  error-message="{{passwordFalse?passwordFalseMess:''}}"
/>

<van-field
  value="{{ password }}"
  label="确认密码"
  placeholder="请确认密码"
  data-name="password"
  type="password"
  required
  bind:change="onFieldChange"
  error-message="{{passwordFalse?passwordFalseMess:''}}"
/>
</van-cell-group>
<button bindtap="check">注册</button>
</view>
